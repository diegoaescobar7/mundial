    color: #ffffff;

}

.gradiente{

    background: #2C3E50;
    background: -moz-linear-gradient(left, #020031 0%, #6d3353 100%);
    background: -webkit-gradient(linear, left top, right top, color-stop(0%,#020031), color-stop(100%,#6d3353));
    background: -webkit-linear-gradient(left,  #020031 0%,#6d3353 100%);
    background: -o-linear-gradient(left,  #020031 0%,#6d3353 100%);
    background: -ms-linear-gradient(left,  #020031 0%,#6d3353 100%);
    background: linear-gradient(to right,  #020031 0%,#6d3353 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#020031', endColorstr='#6d3353',GradientType=1 );

}

.masterBack{

    background: #2C3E50;
